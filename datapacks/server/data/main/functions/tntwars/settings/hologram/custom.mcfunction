kill @e[type=area_effect_cloud,tag=gmh]
execute as @e[type=area_effect_cloud,tag=gamemode_hologram] at @s run particle minecraft:firework ~ ~ ~ 0 0 0 .1 10

execute as @e[type=area_effect_cloud,tag=gamemode_hologram] at @s run summon minecraft:area_effect_cloud ~ ~ ~ {Tags:["gmh"],NoGravity:1b,CustomNameVisible:1b,Duration:2147483647,CustomName:'{"text":"CUSTOM GAME","color":"gold","bold":true}'}

